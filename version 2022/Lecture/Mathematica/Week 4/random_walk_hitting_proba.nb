5a91ca1b9ccd"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"RSolveValue", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"q", "*", 
        RowBox[{"h", "[", 
         RowBox[{"x", "+", "1"}], "]"}]}], "-", 
       RowBox[{"h", "[", "x", "]"}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "q"}], ")"}], "*", 
        RowBox[{"h", "[", 
         RowBox[{"x", "-", "1"}], "]"}]}]}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"h", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"h", "[", "M", "]"}], "\[Equal]", "1"}]}], "}"}], ",", 
   RowBox[{"h", "[", "x", "]"}], ",", "x"}], "]"}]], "Input",
