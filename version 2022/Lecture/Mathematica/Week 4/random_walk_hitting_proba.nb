297ff9d3d5fa"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"RSolveValue", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "/", "2"}], ")"}], "*", 
        RowBox[{"h", "[", 
         RowBox[{"x", "+", "1"}], "]"}]}], "-", 
       RowBox[{"h", "[", "x", "]"}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"(", 
           RowBox[{"1", "/", "2"}], ")"}]}], ")"}], "*", 
        RowBox[{"h", "[", 
         RowBox[{"x", "-", "1"}], "]"}]}]}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"h", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"h", "[", "M", "]"}], "\[Equal]", "1"}]}], "}"}], ",", 
   RowBox[{"h", "[", "x", "]"}], ",", "x"}], "]"}]], "Input",
 CellChangeTimes->{{3.8767192687937326`*^9, 3.8767192765575743`*^9}, {
  3.876719569369812*^9, 3.8767195758540373`*^9}, {3.876720639799506*^9, 
  3.876720673262803*^9}, {3.876829863950644*^9, 3.876829866256073*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"7f8f8072-0144-417c-abf3-9a8aca1ed9f7"],

